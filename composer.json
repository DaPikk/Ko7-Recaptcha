{
  "name": "dapikk/ko7-recaptcha",
  "description": "Simple wrapper for Ko7even MVC for Google's reCAPTCHA library",
  "license": "MIT",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "dapikk/ko7-recaptcha",
        "version": "1.0.0",
        "type": "module",
        "source": {
          "url": "https://github.com/DaPikk/Ko7-Recaptcha.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "extra": {
        "installer-paths": {
            "modules/{$name}": ["type:kohana-module"]
        }
  },
  "require": {
    "php": ">=7.4",
    "google/recaptcha": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.5.2"
  }
}
